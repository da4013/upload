<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">

    <bean name="egovMainImgId1GnrService" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
        <property name="dataSource" ref="egov.dataSource" />
        <property name="strategy"   ref="mainImg1Strategy" />
        <property name="blockSize"  value="1"/>
        <property name="table"      value="COMTECOPSEQ"/>
        <property name="tableName"  value="MAIN_IMG_ID1"/>
    </bean>
    <bean name="mainImg1Strategy" class="egovframework.rte.fdl.idgnr.impl.strategy.EgovIdGnrStrategyImpl">
        <property name="prefix"   value="M_IMG1_" />
        <property name="cipers"   value="7" />
        <property name="fillChar" value="0" />
    </bean>
    
    
    <bean name="egovMainImgId2GnrService" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
        <property name="dataSource" ref="egov.dataSource" />
        <property name="strategy"   ref="mainImg2Strategy" />
        <property name="blockSize"  value="1"/>
        <property name="table"      value="COMTECOPSEQ"/>
        <property name="tableName"  value="MAIN_IMG_ID2"/>
    </bean>
    <bean name="mainImg2Strategy" class="egovframework.rte.fdl.idgnr.impl.strategy.EgovIdGnrStrategyImpl">
        <property name="prefix"   value="M_IMG2_" />
        <property name="cipers"   value="7" />
        <property name="fillChar" value="0" />
    </bean>
    
    
    <bean name="egovMainImgId3GnrService" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
        <property name="dataSource" ref="egov.dataSource" />
        <property name="strategy"   ref="mainImg3Strategy" />
        <property name="blockSize"  value="1"/>
        <property name="table"      value="COMTECOPSEQ"/>
        <property name="tableName"  value="MAIN_IMG_ID3"/>
    </bean>
    <bean name="mainImg3Strategy" class="egovframework.rte.fdl.idgnr.impl.strategy.EgovIdGnrStrategyImpl">
        <property name="prefix"   value="M_IMG3_" />
        <property name="cipers"   value="7" />
        <property name="fillChar" value="0" />
    </bean>
    
</beans>